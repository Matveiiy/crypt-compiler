fn make_cycle() {
    x = array(2);
    y = array(2);
    x[0] = y;
    y[0] = x;
    global = x;
    return global;
}

fn main() {
    global = make_cycle();
    for (i = 0; i < 25; i += 1) {
        tmp = array(1);
    }
